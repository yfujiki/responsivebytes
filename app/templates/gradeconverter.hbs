{{log model._result.names}}
<div class="converter">
    <button type="button" class="btn btn-default btn-lg">
      <span class="glyphicon glyphicon-modal"></span> Converter
    </button>
</div>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            {{#each model._result.names as |name|}}
            <th>{{name}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        <tr>
            {{#each model._result.categories as |category|}}
            <td>{{category}}</td>
            {{/each}}
        </tr>
        {{#each model._result.grades as |grade|}}
        <tr>
            {{#each grade as |grade_per_system|}}
            <td>{{grade_per_system}}</td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>
{{outlet}}
